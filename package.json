{
  "name": "@react-css/flex",
  "version": "1.6.0",
  "description": "A thin wrapper to help make CSS Flexbox simpler and more expressive",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest --coverage",
    "test-watch": "jest --watch",
    "test-watch-all": "jest --watchAll",
    "lint": "tsc --noEmit && eslint '*/**/*.{ts,tsx}' --quiet --fix",
    "build": "tsc",
    "clean": "rm -rf dist/*",
    "release": "yarn prepublishOnly && yarn test && yarn lint",
    "prepublishOnly": "yarn && yarn clean && yarn build",
    "snyk-protect": "snyk protect",
    "prepare": "yarn run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bikk-uk/react-css-flex.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "css",
    "layout",
    "flex",
    "flexbox",
    "flexible",
    "box",
    "styles",
    "simpler",
    "expressive"
  ],
  "author": "bikk-uk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bikk-uk/react-css-flex/issues"
  },
  "homepage": "https://github.com/bikk-uk/react-css-flex#readme",
  "devDependencies": {
    "@testing-library/react": "^12.1.3",
    "@types/jest": "^27.4.0",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "csstype": "^3.0.10",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "snyk": "^1.855.0",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "snyk": true
}
