{
  "name": "@react-css/flex",
  "version": "1.1.0",
  "description": "A thin wrapper to help make CSS Flexbox simpler and more expressive",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest --coverage",
    "test-watch": "jest --watch",
    "test-watch-all": "jest --watchAll",
    "test-coverage": "yarn test && cat ./coverage/lcov.info | coveralls",
    "lint": "tsc --noEmit && eslint '*/**/*.{ts,tsx}' --quiet --fix",
    "build": "tsc",
    "clean": "rm -rf dist/*",
    "release": "yarn && yarn clean && yarn build && yarn test && yarn lint",
    "snyk-protect": "snyk protect",
    "prepare": "yarn run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bikk-uk/react-css-flex.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "css",
    "layout",
    "flex",
    "flexbox",
    "flexible",
    "box",
    "styles",
    "simpler",
    "expressive"
  ],
  "author": "bikk-uk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bikk-uk/react-css-flex/issues"
  },
  "homepage": "https://github.com/bikk-uk/react-css-flex#readme",
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-typescript": "^7.10.1",
    "@testing-library/react": "^10.4.3",
    "@types/jest": "^26.0.3",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "babel-loader": "^8.1.0",
    "coveralls": "^3.1.0",
    "css-loader": "^3.6.0",
    "csstype": "^2.6.10",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^26.1.0",
    "prettier": "^2.0.5",
    "style-loader": "^1.2.1",
    "snyk": "^1.349.0",
    "ts-jest": "^26.1.1",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "snyk": true
}