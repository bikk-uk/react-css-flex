{
  "name": "@react-css/flex",
  "version": "0.1.0",
  "description": "A thin wrapper to help make CSS Flexbox simpler, more expressive, and quicker to use",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest --coverage",
    "test-watch": "jest --watch",
    "test-watch-all": "jest --watchAll",
    "test-coverage": "yarn test && cat ./coverage/lcov.info | coveralls",
    "lint": "tsc --noEmit && eslint '*/**/*.{ts,tsx}' --quiet --fix",
    "build": "tsc",
    "clean": "rm -rf dist/*",
    "release": "yarn && yarn clean && yarn build && yarn test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bikk-uk/react-css-flex.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "css",
    "layout",
    "flex",
    "flexbox",
    "flexible",
    "box",
    "styles",
    "simpler",
    "expressive"
  ],
  "author": "bikk-uk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bikk-uk/react-css-flex/issues"
  },
  "homepage": "https://github.com/bikk-uk/react-css-flex#readme",
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-typescript": "^7.10.1",
    "@testing-library/react": "^10.2.1",
    "@types/jest": "^25.2.3",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "babel-loader": "^8.1.0",
    "coveralls": "^3.1.0",
    "css-loader": "^3.5.3",
    "csstype": "^2.6.10",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "style-loader": "^1.2.1",
    "ts-jest": "^26.1.0",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}