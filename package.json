{
  "name": "@react-css/flex",
  "version": "1.4.4",
  "description": "A thin wrapper to help make CSS Flexbox simpler and more expressive",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest --coverage",
    "test-watch": "jest --watch",
    "test-watch-all": "jest --watchAll",
    "lint": "tsc --noEmit && eslint '*/**/*.{ts,tsx}' --quiet --fix",
    "build": "tsc",
    "clean": "rm -rf dist/*",
    "release": "yarn prepublishOnly && yarn test && yarn lint",
    "prepublishOnly": "yarn && yarn clean && yarn build",
    "snyk-protect": "snyk protect",
    "prepare": "yarn run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bikk-uk/react-css-flex.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "css",
    "layout",
    "flex",
    "flexbox",
    "flexible",
    "box",
    "styles",
    "simpler",
    "expressive"
  ],
  "author": "bikk-uk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bikk-uk/react-css-flex/issues"
  },
  "homepage": "https://github.com/bikk-uk/react-css-flex#readme",
  "devDependencies": {
    "@testing-library/react": "^11.2.6",
    "@types/jest": "^26.0.22",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "csstype": "^3.0.7",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "snyk": "^1.521.0",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "snyk": true
}
